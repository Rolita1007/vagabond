<div id="cities-view-container">
    <h1>ALL POSTS</h1>
    <ul>
        <% bootstrap_form_tag @posts.each do |post| %>
            <li>
                <h2>
                    <%= post.content %>
                </h2>
            </li>
            <% end %>

    </ul>
    <li>
        <%= link_to "new post", new_city_post_path%>
    </li>
</div>

<div class="btn btn-primary">
  <%= link_to 'Edit', edit_product_path(@product), :class => "btn" %> |
  <%= link_to 'Back', products_path, :class => "btn"%>
</div>
